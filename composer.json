{
    "name": "tzmfreedom/phpstan-visible-for-testing",
    "description": "PHPStan custom rules to ensure that VisibleForTesting annotated public methods are called in private/protected scopes outside of the test environment",
    "keywords": ["PHPStan", "VisibleForTesting", "static analysis"],
    "type": "phpstan-extension",
    "license": "MIT",
    "homepage": "https://github.com/tzmfreedom/phpstan-visible-for-testing",
    "autoload": {
        "psr-4": {
            "Tzmfreedom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tzmfreedom\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "tzmfreedom",
            "email": "makoto_tajitsu@hotmail.co.jp"
        }
    ],
    "require": {
        "phpstan/phpstan": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
